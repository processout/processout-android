package com.processout.sdk.netcetera.threeds.core

import com.netcetera.threeds.sdk.api.configparameters.builder.ConfigurationBuilder
import com.processout.sdk.netcetera.threeds.core.annotation.ProcessOutInternalApi

internal object MainSecrets {

    init {
        System.loadLibrary("po.netcetera.3ds")
    }

    @JvmName(name = "a0")
    external fun getApiKey(): String
}

@ProcessOutInternalApi
fun ConfigurationBuilder.poApiKey(): ConfigurationBuilder =
    apiKey(MainSecrets.getApiKey())
